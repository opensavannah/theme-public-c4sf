footer
  .container
    .row.partners-title
      .col-sm-12
        h3 Partners
    .row.partners-logos.ta-c
      each sponsor in brigade.sponsors
        if (!(sponsor.main) && (sponsor.link !== "undefined"))
          .col-md-4.d-ib
            a(href=sponsor.link target="_blank")
              img(src=sponsor.image, alt=sponsor.name)
        else
          .col-md-4.d-ib
            a()
              img(src=sponsor.image, alt=sponsor.name)
  .row.footer-nav
    .col-sm-12
      ul
         li#menu-item-385.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-385
          a(href='https://twitter.com/open_savannah') 
            i.fa.fa-twitter.fa-lg(aria-hidden='true')
         li#menu-item-385.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-385
          a(href='https://github.com/opensavannah') 
            i.fa.fa-github.fa-lg(aria-hidden='true')
         li#menu-item-385.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-385
          a(href='https://facebook.com/opensavannah') 
            i.fa.fa-facebook.fa-lg(aria-hidden='true')
         li#menu-item-385.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-385
          a(href='https://techsav.slack.com/messages/opensavannah') 
            i.fa.fa-slack.fa-lg(aria-hidden='true')            
         li#menu-item-385.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-385
          a(href='https://medium.com/@opensavannah') 
            i.fa.fa-medium.fa-lg(aria-hidden='true')
         each external in brigade.theme.page.external
          li
            a(href=external.link, target=external.target)=external.name
         if brigade.theme.page.about
          li
            a(href='/contact') Contact

      
         li.float.right
          a(href='https://brigadehub.github.io/') powered by BrigadeHub
